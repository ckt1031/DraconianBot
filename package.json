{
  "private": true,
  "name": "draconian",
  "version": "2.1.0",
  "description": "Multi-Purpose & Productivity concentrated Discord BOT.",
  "license": "MIT",
  "author": "ckt1031",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "dev": "nodemon",
    "format": "prettier -w .",
    "merge-config": "node --loader @esbuild-kit/esm-loader tools/merge-config.ts",
    "start": "cross-env NODE_ENV=production node --loader @esbuild-kit/esm-loader src/index.ts",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@discordjs/builders": "1.9.0",
    "@discordjs/rest": "2.4.0",
    "@esbuild-kit/esm-loader": "2.6.5",
    "@vitalets/google-translate-api": "9.2.0",
    "axios": "1.7.9",
    "canvas": "2.10.2",
    "chalk": "5.3.0",
    "cross-env": "7.0.3",
    "crypto-js": "4.2.0",
    "dayjs": "1.11.13",
    "deepmerge": "4.3.1",
    "discord-api-types": "0.37.105",
    "discord.js": "14.16.3",
    "dotenv": "16.4.5",
    "fastify": "5.1.0",
    "gifencoder": "2.0.1",
    "glob": "11.0.0",
    "mongoose": "8.8.1",
    "ms": "2.1.3",
    "node-cache": "5.1.2",
    "pidusage": "3.0.2",
    "string-similarity": "4.0.4",
    "xml2js": "0.6.2",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/crypto-js": "4.2.2",
    "@types/gifencoder": "2.0.3",
    "@types/ms": "0.7.34",
    "@types/node": "22.9.0",
    "@types/pidusage": "2.0.5",
    "@types/string-similarity": "4.0.2",
    "@types/xml2js": "0.4.14",
    "nodemon": "3.1.7",
    "typescript": "5.6.3"
  },
  "engines": {
    "node": ">=16.9.0",
    "npm": ">=7"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "delay": 2500,
    "ignore": [
      "dist",
      "temp",
      "node_modules"
    ],
    "exec": "node --loader @esbuild-kit/esm-loader --inspect src/index.ts"
  },
  "packageManager": "npm@10.9.0"
}
